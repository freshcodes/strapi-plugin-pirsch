{
  "name": "@fresh.codes/strapi-plugin-pirsch",
  "description": "A Strapi plugin to display an embedded Pirsch dashboard.",
  "license": "ISC",
  "author": "Fresh Codes <hello@fresh.codes>",
  "version": "1.0.3",
  "keywords": [
    "strapi",
    "pirsch",
    "analytics",
    "plugin"
  ],
  "strapi": {
    "kind": "plugin",
    "name": "pirsch",
    "displayName": "Pirsch",
    "description": "A Strapi plugin to display an embedded Pirsch dashboard."
  },
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write --ignore-unknown .",
    "prepare": "husky",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.29",
    "@strapi/icons": "^2.0.0-rc.29",
    "react-intl": "^7.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.20.0",
    "@strapi/typescript-utils": "^5.20.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.18.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19"
  }
}
